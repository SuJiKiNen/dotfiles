# shellcheck disable=SC2148
# shellcheck disable=SC1090

if [ -f ~/.bashrc ]; then
    source ~/.bashrc
fi

if [ -f ~/.bash_plugins ]; then
    source ~/.bash_plugins
fi

# force utf-8
if [[ "$OSTYPE" == "msys" ]]; then
   chcp.com 65001 > /dev/null 2>&1
fi

